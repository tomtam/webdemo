.work-block {
    //height: 1200px;
}
.checkInfo-block {
    display: none;
    p {}
    a {
        text-decoration: underline;
        color: #036;
    }
    .title {
        padding: 42px;
        font-weight: bold;
        font-size: 16px;
        color: #666;
        text-align: center;
    }
    li {
        width: 200px;
        height: 400px;
        padding: 28px 0 0 28px;
        background: #fff;
        display: inline-block;
        border: 1px solid red;
        border: 1px solid #fff;
        border-radius: 3px;
        vertical-align: top;
        margin-right: 8px;
    }
    .cur {
        border: 2px solid #036;
        border-radius: 3px;
    }
    .plain {
        border: 1px solid #fff;
        border-radius: 3px;
    }
    .empty {
        border: 2px dashed #999;
        border-radius: 3px;
        background: #ddd;
        //visibility: hidden;
    }
    .msg-block {
        padding: 10px 0;
        line-height: 20px;
        height: 40px;
        label {
            color: #999;
        }
        p {
            color: #333;
            width: 190px;
        }
        .phone {
            color: #000;
            font-weight: bold;
        }
        .ppcolor {
            font-weight: bold;
            color: #1023af;
        }
    }
    .warnmsg {
        text-align: center;
        font-size: 12px;
        color: #900;
        padding: 30px;
    }
    .button-group {
        .button {
            cursor: pointer;
            //width: 90px;
            background: #ccc;
            display: inline-block;
            border-radius: 3px;
            border: 1px solid #ccc;
            color: #036;
            line-height: 57px;
            height: 57px;
            text-align: center;
            margin-right: 9px;
        }
        .goback {
            width: 224px;
            height: 57px;
            color: #036;
        }
        .next {
            width: 476px;
            height: 57px;
            color: #036;
            background: #fff;
            font-weight: bold;
        }
        .create {
            width: 222px;
            height: 57px;
            color: #999;
        }
    }
}
